<?xml version="1.0"?>
<!-- originally based on McBen's findings at
     https://github.com/McBen/FDB_Extractor2/wiki/db_exptable -->
<dbformat>
  <field offset="0000" name="guid" type="u32" size="4" />
  <field offset="0004" name="level" type="u32" size="4" />
  <field offset="0008" name="lvlup_ratio" type="u32" size="4" /> <!-- how many mobs you need to kill for lvlup -->
  <field offset="000c" name="unk1" type="u32" size="4" />
  <field offset="0010" name="unk2" type="u32" size="4" />
  <field offset="0014" name="unk3" type="u32" size="4" />
  <field offset="0018" name="quest_xp_base" type="u32" size="4" />
  <field offset="001c" name="quest_money_base" type="u32" size="4" />
  <field offset="0020" name="exp" type="float" size="4" /> <!-- how much exp from one killed mob -->
  <field offset="0024" name="skill_cost" type="u32" size="4" /> <!-- talent points for skills: ExpTable[level + Spell.expcostrate] -->
  <field offset="002c" name="unk4" type="float" size="4" />
  <field offset="0030" name="unk5" type="float" size="4" />
  <field offset="0034" name="unk6" type="u32" size="4" />
  <field offset="0038" name="unk7" type="u32" size="4" />
  <field offset="003c" name="unk8" type="u32" size="4" />
  <field offset="0040" name="unk9" type="u32" size="4" />
  <field offset="0044" name="unk10" type="float" size="4" />
  <field offset="0048" name="unk11" type="u32" size="4" />
  <field offset="004c" name="unk12" type="u32" size="4" />
  <field offset="0050" name="unk13" type="u32" size="4" />
  <field offset="0054" name="unk14" type="float" size="4" />
</dbformat>
